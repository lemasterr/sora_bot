{
  "name": "sora-suite-electron",
  "version": "1.0.0",
  "description": "Electron + React shell for the Sora Suite Python automation workers",
  "main": "dist-electron/main.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:renderer\" \"npm:dev:electron\" \"npm:dev:app\"",
    "dev:renderer": "vite dev --host 0.0.0.0 --port 5173",
    "dev:electron": "tsc -p electron/tsconfig.json --watch",
    "dev:app": "wait-on dist-electron/main.js && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main.js",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "lint": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "electron": "^32.2.8",
    "lucide-react": "^0.469.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "wait-on": "^8.0.0"
  }
}
